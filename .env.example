# API Keys for LLM Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# LLM Provider Configuration
# GATHER_PROVIDER=openrouter  # Options: openrouter, groq (default: openrouter)
# GATHER_MODEL=openai/gpt-oss-120b  # Model to use for gathering/segmentation

# EVALUATION_PROVIDER=openrouter  # Options: openrouter, groq (default: openrouter)
# EVALUATION_MODEL=qwen/qwen3-32b  # Model to use for evaluation/scoring

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# PostgreSQL Database Configuration
HIREVOX_DATABASE_URL=postgresql://username:password@hostname:port/database_name